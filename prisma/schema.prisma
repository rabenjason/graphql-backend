generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Utilisateur {
  id_utilisateur String  @id 
  nom            String
  prenom         String
  email          String  @unique
  motDePasse     String
  telephone      String?
  facebook       String?
  whatsapp       String?
  photo          String?
}
